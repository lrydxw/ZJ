    <script>
        var param = getid();
        if(param){
        	window.location = "component/main/index.html?"+param;
        }else{
        	window.location = "component/main/index.html";
        }
        function getid() {
            var idUrl = document.URL;
            var num = idUrl.indexOf("?")
            if(num == -1){//不带参
                return '';
            }
            idUrl = idUrl.substr(num + 1);
            return idUrl;
        }
