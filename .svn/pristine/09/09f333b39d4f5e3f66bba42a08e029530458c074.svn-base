html {
  width: 100%;
  height: 100%;
  font-size: 100px;
   }

body {
  width: 100%;
  height: 100%;
  font-size: 24px;
  background: #efefef;
   }


.form{
  width:100%;
  height:100%;
  background:#efefef;
  overflow:auto;
}
 .SQ-title{

font-size:16px;
color:#20304b;
letter-spacing:0;
text-align:left;
display:block;
width:87px;
position:absolute;
line-height: 24px;
height:24px;
 }
.input-box{
  width: 100%;
  padding:0 40px 0 86px;
  box-sizing: border-box;
}

.form-li{
  background: #fff;
  height:24px;
  padding:12px;
  margin-top:1px;
  position:relative;


}
.li-first{
  margin-top:10px;
}
.shuru{
  display: block;
  font-size:16px;
  text-align:left;
  border:none;
  width:100%;
  box-sizing: border-box;
  outline: none;
  line-height: 24px;
  height:24px;
}

.shuru::placeholder{
  color:#bcc0c9;
}
.shuru::-webkit-input-placeholder{
  color:#bcc0c9;
}
.img-right{
width:40px;
float:right;
}
.YZ1{
background:#f7f7f7;
border:1px solid #dfdfdf;
width:100%;
height:55px;
margin-top:36px;
text-indent:13px;
font-size:16px;
line-height:21px;
text-align:left;
}
.YZ1::placeholder,.YZ2::placeholder,.YZ1::-webkit-input-placeholder,.YZ2::-webkit-input-placeholder{
  color:#bcc0c9;
}
.YZ2{
background:#f7f7f7;
border:1px solid #dfdfdf;
width:100%;
height:192px;
margin-top:10px;
padding-left:13px;
padding-top:10px;
padding-right:14px;
box-sizing: border-box;

font-size:16px;
line-height:21px;
text-align:left;
}
.li-last{
  height:auto;
  margin-top:9px;
}


.g-head{
  background: #fff;
  height:105px;
  margin-top:54px;

  border-bottom:1px solid #efefef;
}

.form-box{
  margin-top:10px;
  overflow: hidden;
}
.btn{
background:#0093ff;
border:0;
height:48px;
line-height: 48px;
font-size:18px;
color:#ffffff;
text-align:center;
margin-top:5px;
width:100%;
position:absolute;
bottom:0;
}

.btn-off{
  background:#dfdfdf
}