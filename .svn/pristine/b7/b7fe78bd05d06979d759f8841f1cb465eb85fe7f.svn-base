.bodtom-box {
    z-index: 666;
    position: fixed;
    bottom: 0rem;
    width: 100%;
    padding: .375rem 0 .375rem .9375rem;
    background-color: #3b98e0;
}

.icon {
    display: inline-block;
    background-repeat: no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
}

.logobot-icon {
    margin-right: .625rem;
    width: 2.562rem;
    height: 2.562rem;
    background-image: url(../img/broadcast-logo-bot.png);
}

.bot-title {
    display: inline-block;
    text-align: left;
    font-size: .9375rem;
    color: #FFFFFF;
    vertical-align: top;
    margin-top: .86rem;
    line-height: .9375rem;
}

.commonButton {
    float: right;
    height: 2rem;
    width: 6rem;
    margin-right: 2rem;
    border: .0625rem solid #FFFFFF;
    background-color: #FFFFFF;
    color: #3b98e0;
    font-size: .9375rem;
    line-height: .9375rem;
    border-radius: .125rem;
    margin-top: .4rem;
    outline: none;
}

.b-zone4{
    background: url(../img/lbc-phone.png) no-repeat;
}

.telno{
    color: #DADADA;
}
.telno .b-zone4{
    background-image: url(../img/lbc_zone4.png);
}

.zheZhao {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.15);
    z-index: 9998;
}

#jCall {
    display: none;
    position: fixed;
    top: 50%;
    left: 50%;
    z-index: 9999;
    width: 80%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #fff;
    -webkit-border-radius: 6px;
    border-radius: 6px;
}

#jCall a {
    text-decoration: none;
    color: #000000;
}

#jCall p {
    text-align: center;
    height: 3rem;
    line-height: 3rem;
    border-bottom: 1px solid #E0DDDD;
    font-size: 1.25rem;
}

#cancle {
    text-align: center;
    height: 3rem;
    line-height: 3rem;
    font-size: 1.25rem;
}

.pad-top {
    padding-top:1rem;
}

.fontAnnotation {
    color: #999;
}

#J-address {
    line-height: 1.3;
}