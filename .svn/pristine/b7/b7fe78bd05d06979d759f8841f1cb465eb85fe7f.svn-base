@charset "utf-8";
/*class  -连接*/
@media screen and (max-width: 374px) {
	*{font-size: 14px;line-height: 14px;}
}
@media (min-width:375px) and (max-width: 413px) {
	*{font-size: 14px;line-height: 14px;}
}
.bodyTop{z-index: 2;}
.commonButton{
    height:3.571rem;
}
.fontOrange {
    color: #ff6230;
}
.common-div{
    padding: 0 0 0.357rem 0;
    z-index: 1;
    position: relative;
}
.backg-div{
    position: absolute;
    left: 0;
    padding: 0 0.714rem;
    height: 100%;
    width: 100%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    z-index: -1;
}
.backg-div .div1{
    float: left;
    width: 25.5%;
    height: 100%;
    padding-left:0.357rem;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.backg-div .div2{
    float: left;
    width: 74.5%;
    height: 100%;
    padding-left:0.785rem;
    padding-right:0.357rem;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.backg{
    height: 100%;
    width: 100%;
    background-color: #ff592c;
}
.coupon-card{
    /*display: flex;*/
    width: 100%;
    margin: 0.714rem 0 0.714rem 0px;
    height: 8.42rem;
    background-size: auto 100%;
    white-space:nowrap;
}
.coupon-left{
    /*flex-basis: 25.5%;*/
    float: left;
    display: inline-block;
    width: 25.5%;
    line-height: 8.42rem;
    text-align: center;
    font-size: 2.14rem;
    color: #ffeb37;
    background: url(../img/couponimg_side.png) no-repeat 0px 0px;
    background-size: auto 100%;
}
#J-quota{
    position: absolute;
    margin-top: 4.14rem;
    font-size: 5rem;
    color: #fff;
    text-align: center;
    white-space:normal;
    width:100%;
    text-align:center;
}
.coupon-right{
    display: inline-block;
    /*flex-basis: 74.5%;*/
    float: left;
    width: 74.5%;
    height: 100%;
    font-size: 1rem;
    color: #FFF;
    line-height: 1.5;
    background-image: url(../img/couponimg_middle.png),url(../img/couponimg_side_r.png);
    background-repeat: no-repeat;
    background-position: 0 0,right top;
    background-size: auto 100%;
}
.coupon-right div{
    padding-top: 1.428rem;
    padding-left: 12%;
}
.ft16{
    font-size: 1.14rem;
    letter-spacing: 0.142rem;
}
.received{
    /*margin-right: 10%;*/
    /*float: right;*/
    position: absolute;
    right: 10%;
    bottom: 14px;
}
.ft-orange{
    color: #ff6100;
}
.coupon-info{
    margin-left: 1.428rem;
    margin-right: 0.714rem;
    line-height: 1.5;
}
.deal{
    border-left: 1px solid #DADADA;
    /* display: flex; */
    /*width: 4.285rem;*/
    /*height: 2.5rem;*/
    background-color: #fff;
    position: absolute;
    right: 0px;
    top: 5rem;
}
.deal li{
    width:100%;
    height:100%;
}
.deal .li-1{
    border-right: 1px solid #DADADA;
}
.deal i{
    display: inline-block;
    width: 1.714rem;
    height: 1.714rem;
    background-size: 1.714rem 1.714rem;
    vertical-align: top;
    margin: 0.714rem 1.285rem;
}
.deal span{
    display: inline-block;
    height: 2.571rem;
    vertical-align: top;
}

.pl10{
    padding-left: 0.714rem;
}
.shopinfo{
    background-color: #FFF;
}
.shopinfo .top{
    margin: 0.714rem 0 1rem 1.428rem;
    border-bottom: 1px solid #E0DDDD;
    position: relative;
    height: 4.714rem;
}
.border-top{
    border-top:  0.714rem solid #EFEFEF;
}
.border-bottom{
    border-bottom:  0.714rem solid #EFEFEF;
}

.active{
    background-color: #ff6230;
    color: #FFF;
}
.black,.black:hover{
    background-color: #CBCBCB;
    border: none;
    outline: none;
    text-align: center;
    line-height: 3.428rem;
}
.cdbg{
    box-sizing: border-box;
    width: 100%;
    padding: 0.714rem 0.714rem;
    background-color: #f7f7f7;
    position: fixed;
    bottom: 0;
}
.more{
    line-height: 3.21rem;
    color: #3295f2;
    position: relative;
    margin-bottom: 4.71rem;
}
.more span{
    padding-left: 0.714rem;
}
.more i{
    position: absolute;
    height: 3.21rem;
    width: 2.14rem;
    right: 0.714rem;
    top: 1.14rem;
    display: inline-block;
    background: transparent url(../img/more-icon-right.png) no-repeat 0px 0px;
    background-size: 36%;
}
.yj-div{
    float: right;
    margin-right: 1.428rem;
    font-size: 1rem;
}
.online-pay{
    position: absolute;
    color: #FFFFFF;
    width:25.5%;
    text-align: center;
    top: 6.07rem;
    font-size: 0.857rem;
    display: none;
}
.coupon-detail{
    width:100%;
    height:14.285rem;
    background: url("../img/lbc_coupondetail.jpg") no-repeat;
    background-size:100% 14.285rem;
    position: relative;
}
#s-money{
    display: block;
    padding-left:6.428rem;
    margin-top: 0.3rem;
}
.address{
    background: url("../img/lbc_address.png") top left no-repeat;
    text-indent:1.785rem;
    background-size:1.285rem 1.285rem,0.642rem 1.14rem;
    font-size:0rem;
    margin:0 1.428rem;
    width: 75%;
}
#J-merchantname-2{
    /*font-weight: bold;*/
    padding-left: 6.428rem;
    font-size: 1.285rem;
}
dl.common-div{
    margin: 0;
    padding-top: 12px;
}
dd{
    margin: 0;
    padding: 0.285rem 0 0.857rem 0;
}
dt{
    color:#999;
}
