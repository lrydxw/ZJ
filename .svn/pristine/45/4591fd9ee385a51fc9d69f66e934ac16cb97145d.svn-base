#cercle {
	margin: 5px;
	height: 50px;
	width: 50px;
	position: absolute;
	top: 45px;
	left: 45px;
	border-radius: 50%;
	background: -webkit-linear-gradient(#999, #666);
	background: linear-gradient(#999, #666);
	animation: turnCercle 5s infinite;
	-webkit-animation: turnCercle 5s infinite;
	animation-timing-function: linear;
	-webkit-animation-timing-function: linear;
}
@-webkit-keyframes turnCercle {
	0% {
		-webkit-transform: rotate(0deg);
	}
	100% {
		-webkit-transform: rotate(5040deg);
	}
}
#cercleCache {
	margin: 5px;
	height: 40px;
	width: 40px;
	position: absolute;
	border-radius: 50%;
	background-color: #fff;
	z-index: 5;
}
