.box-mt{
    /*margin-top: 1rem;*/
}
.reg-info {
    line-height: 2.593rem;
    font-size: 0.813rem;
    color: #ff871c;
    text-indent: 0.938rem;
    background-color: #fff3b9;
}

.reg-info i {
    position: absolute;
    right: 0.938rem;
    width: 0.438rem;
    height: 2.593rem;
    background: transparent url(../img/fs-arrow.png) no-repeat center center;
    -webkit-background-size: 100% auto;
    background-size: 100% auto;
}

.fs-tab {
    width: 100%;
    padding-top: 3px;
    height: 2.438rem;
    background-color: #fff;
    text-align: center;
    border: 1px solid transparent;
}

.fs-tab > .float_two {
    line-height: 2.438rem;
    text-align: center;
    color: #666;
}


.tab-text {
    display: inline-block;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    vertical-align: top;
    padding: 0 3px;
    line-height: 2.438rem;
    font-size: 0.875rem;
}

.float_two.active > .tab-text {
    border-bottom: 2px solid #3b98e0;
    color: #3b98e0;
}

/*流量当月概况：begin*/
.flow-box {
    padding: 1.094rem 1.844rem;
}

.fs-flow {
    color: #FFF;
}

.flow-content {
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    text-align: center;
}

.flow-content > .fc-2 {
    margin: 0.4rem auto 1rem auto;
}

.flow-content > p {
    font-size: 0.75rem;
}

.flow-content > .fc-2 {
    font-size: 1.5rem;
}

.flow-des {
    height: 3.188rem;
    border-bottom-left-radius: 0.5rem;
    border-bottom-right-radius: 0.5rem;
    background-color: #fff;
}

.fc-des {
    padding: 0.625rem;
    color: #999;
    font-size: 0.75rem;
}

.chart-box {
    margin: 1.8rem 1.8rem 0 1.8rem;
}

.fc-chart {
    position: relative;
    height: 0.312rem;
    -webkit-border-radius: 0.156rem;
    border-radius: 0.156rem;
    background-color: #0b64a8;
    text-align: left;
}

.fc-bar {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 0;
    background-color: #ffd824;
    -webkit-border-radius: 0.156rem;
    border-radius: 0.156rem;
    -webkit-transition: width ease-in-out 1s 0.7s;
    transition: width ease-in-out 1s 0.7s;
}

.fc-circle {
    position: absolute;
    top: -0.144rem;
    right: -0.156rem;
    height: 0.6rem;
    width: 0.6rem;
    -webkit-border-radius: 0.5rem;
    border-radius: 0.5rem;
    background-color: inherit;
}

.fc-circle-s {
    position: absolute;
    top: 0.062rem;
    width: 0.188rem;
    height: 0.188rem;
    -webkit-border-radius: 0.188rem;
    border-radius: 0.188rem;
    background-color: #459de1;
}
.l33{
    left: 33.3%;
}
.fcst{
    top: 0.7rem;
    left: -0.7rem;
    font-size: 0.75rem;
    font-style: normal;
}
/*流量当月概况：end*/

/*流量历史：begin*/
.flow-history li {
    position: relative;
    height: 3rem;
    line-height: 3rem;
    color: #999;
    /*border-bottom: 1px solid #DADADA;*/
}

.flow-history .left,
.flow-history .center,
.flow-history .right{
    font-size: 0.875rem;
}
.fh-abs {
    position: absolute;
    line-height: 3rem;
    top: 0;
}

.flow-history .left {
    width: 2.6rem;
    left: 0;
}

.flow-history .right {
    width: 5.313rem;
    right: 0;
}

.flow-history .center {
    line-height: 3rem;
    margin: 0 5.313rem 0 2.281rem;
}
.common-remBox{
    /*border-top: 1px solid #dfdfdf;*/
    background-color: #fff;
}
.li-btn{
    display: inline-block;
    width: 4.9rem;
    line-height: 1.6rem;
    text-align: center;
    color: #3b98e0;
    font-size: 0.875rem;
    background-color: #fff;
    outline: none;
    border: 1px solid #3b98e0;
    -webkit-border-radius: 0.9rem;
    border-radius: 0.9rem;
}
.li-btn.gray{
    color: #999;
    border: 1px solid #999;
}
.load-more{
    color: #3b98e0;
}
/*流量历史：end*/

/*免费电话：begin*/
.simple-btn {
    display: inline-block;
    margin-top: 1.24rem;
    line-height: 1.8rem;
    padding: 0 1.25rem;
    text-align: center;
    text-decoration: none;
    color: #fff;
    outline: none;
    border: 1px solid currentColor;
    -webkit-border-radius: 0.9rem;
    border-radius: 0.9rem;
    margin-bottom:1.24rem;
}

.ft-detail {
    padding: 0.48rem 0.6rem 0rem 0.6rem;
    border-bottom:1px solid #DFDFDF;
}

.ft-li {
    margin-top: 0.5rem;
    height: 1.94rem;
}

.ft-li-span {
    line-height: 1.94rem;
    color:#68758e;
}

.after-scale {
    position: relative;
}

.after-scale:after {
    content: "";
    position: absolute;
    bottom: -3px;
    left: 0px;
    right: 0px;
    border-bottom: 1px solid #dfdfdf;
    -webkit-transform: scaleY(.5);
    -webkit-transform-origin: 0 0;
}

/*免费电话：end*/

.after-scale {
    position: relative;
}

.after-scale:after {
    content: "";
    position: absolute;
    bottom: -3px;
    left: 0px;
    right: 0px;
    border-bottom: 1px solid #dfdfdf;
    -webkit-transform: scaleY(.5);
    -webkit-transform-origin: 0 0;
}
.before-scale {
    position: relative;
}

.before-scale:before {
    content: "";
    position: absolute;
    top: -1px;
    left: 0px;
    right: 0px;
    border-bottom: 1px solid #dfdfdf;
    -webkit-transform: scaleY(.5);
    -webkit-transform-origin: 0 0;
}

