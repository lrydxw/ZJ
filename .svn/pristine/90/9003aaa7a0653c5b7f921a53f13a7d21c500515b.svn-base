.mt49{
	margin-top: 48px;
	padding-top: 1px;
}
.trade_data{
	padding: 0 0.9375rem;
	margin: 0.625rem 0;
	border-bottom: 1px solid #DFDFDF;
	border-top:1px solid #DFDFDF;
	background: #FFFFFF;
}
.trade_evalaute{
	position: relative;
	border-bottom: 1px solid #DFDFDF;
	padding: 0.9375rem 0;
}
.trade_detail{
	padding: 0.5rem 0;
}
.thephoto{
	position: absolute;
	width: 2.5rem;
	height: 2.5rem;
	background: url(../img/thephoto.png)no-repeat;
	background-size:contain ;
}
#thestar{
	margin-left: 2.968rem;
	position: relative;
	margin-top: 0.375rem;
}
.thename{
	margin-left: 2.968rem;
	font-size: 0.9375rem;
	min-height: 0.9375rem;
}
.thestars{
	background: url(../img/trade-starok5.png)no-repeat;
	width: 0.875rem;
	height: 0.812rem;
	background-size:contain ;
	display: inline-block;
}
.thestarsno{
	background: url(../img/trade-starno.png)no-repeat;
	width: 0.875rem;
	height: 0.812rem;
	background-size:contain ;
	display: inline-block;
}
.thetime{
	right:0;
	top: -27px;
	font-size: 0.6rem;
	position: absolute;
	color:#c3c9d5;
	background: url("../img/time.png") no-repeat left center;
	text-indent: 10px;
}
.theevaluate{
	margin-top: 0.6rem;
	color: #252525;
}
.address_info{
	font-size: 0.875rem;
	color: #999999;
	/*width: 100%;*/
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}
.goodsinfo{
	color: #999999;
	font-size: 0.625rem;
}
.loadmore {
	width: 100%;
	height: 16px;
	line-height: 16px;
	margin-bottom: 10px;
	color: #3295F2;
	z-index: 9999;
	bottom: 0;
	font-size: 16px;
	text-align: center;
	border-top: 0;
}
.none_background{
	background-image: url(../img/no-info-icon-new-s.png);
}