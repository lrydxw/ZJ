.tf-box {
	position: fixed;
	display: none;
	background-color: #FFF;
	border-bottom: 0px;
	bottom: 0px;
	min-height:100px;
	z-index: 102;
	width: 100%;
}
.tf-top{
	font-size: 1.0rem;
	color: #252525;
	border-bottom: 1px solid #DFDFDF;
	background-color: #F7F7F7;
	text-align: center;
	line-height: 3.0rem;
	height: 3.0rem;
}
.tf-cancel{
	color: #999999;
	font-size: 1.0rem;
	line-height: 3.0rem;
	position: absolute;
	left: 0.75rem;
}
.tf-con{
	padding: 0.75rem 0 0 0;
}
.tf-div1{
	color: #999999;
	font-size: 0.75rem;
	text-align: center;
}
.tf-div2{
	padding: 1.25rem;
}
.tf-img {
	width: 100%;
	height: auto;
}
.tf-btn{
	text-align: center;
	color: #3B98E0;
	font-size: 1.0rem;
	border-top: 1px solid #DFDFDF;
	height: 3.0rem;
	line-height: 3.0rem;
	font-weight: bold;
}
.tf-bg{
	display: none;
	position: absolute;
	background-color: #000000;
	opacity: 0.4;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 101;
}
.hid{
	display: none;
}
